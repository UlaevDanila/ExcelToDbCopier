@model IEnumerable<TestTask.User.User>

<table class="table">
    <thead>
    <tr>
        <th>@Html.DisplayNameFor(model => model.PK_Id)</th>
        <th>@Html.DisplayNameFor(model => model.CardCode)</th>
        <th>@Html.DisplayNameFor(model => model.LastName)</th>
        <th>@Html.DisplayNameFor(model => model.FirstName)</th>
        <th>@Html.DisplayNameFor(model => model.Surname)</th>
        <th>@Html.DisplayNameFor(model => model.PhoneMobile)</th>
        <th>@Html.DisplayNameFor(model => model.Email)</th>
        <th>@Html.DisplayNameFor(model => model.GenderId)</th>
        <th>@Html.DisplayNameFor(model => model.Birthday)</th>
        <th>@Html.DisplayNameFor(model => model.City)</th>
        <th>@Html.DisplayNameFor(model => model.Pincode)</th>
        <th>@Html.DisplayNameFor(model => model.Bonus)</th>
        <th>@Html.DisplayNameFor(model => model.Turnover)</th>
        <th>Action</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
    <tr>
        <td>@Html.DisplayFor(modelItem => item.PK_Id)</td>
        <td>@Html.DisplayFor(modelItem => item.CardCode)</td>
        <td>@Html.DisplayFor(modelItem => item.LastName)</td>
        <td>@Html.DisplayFor(modelItem => item.FirstName)</td>
        <td>@Html.DisplayFor(modelItem => item.Surname)</td>
        <td>@Html.DisplayFor(modelItem => item.PhoneMobile)</td>
        <td>@Html.DisplayFor(modelItem => item.Email)</td>
        <td>@Html.DisplayFor(modelItem => item.GenderId)</td>
        <td>@Html.DisplayFor(modelItem => item.Birthday)</td>
        <td>@Html.DisplayFor(modelItem => item.City)</td>
        <td>@Html.DisplayFor(modelItem => item.Pincode)</td>
        <td>@Html.DisplayFor(modelItem => item.Bonus)</td>
        <td>@Html.DisplayFor(modelItem => item.Turnover)</td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.PK_Id })

        </td>
    </tr>
    }
    </tbody>
</table>